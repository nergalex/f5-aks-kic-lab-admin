{
  "metadata": {
    "name": "{{ extra_app.name }}{{ extra_volterra_site_id }}-{{ extra_env }}-devportal",
    "namespace": "{{ extra_volterra_namespace }}",
    "labels": {
      "owner": "{{ extra_owner_email.split('@')[0] }}",
      "project": "cloudbuilder"
    },
    "description": "NGINX workshop",
    "disable": false
  },
  "spec": {
    "origin_servers": [
      {
        "k8s_service": {
          "service_name": "nginx-devportal.{{ extra_volterra_namespace }}",
          "site_locator": {
            "virtual_site": {
              "tenant": "{{ extra_volterra.tenant.full }}",
              "namespace": "{{ extra_volterra_namespace }}",
              "name": "cloudbuilder-landing-zones"
            }
          },
          "vk8s_networks": {}
        },
        "labels": {}
      }
    ],
    "no_tls": {},
    "port": 80,
    "same_as_endpoint_port": {},
    "healthcheck": [
      {
        "tenant": "{{ extra_volterra.tenant.full }}",
        "namespace": "{{ extra_volterra_namespace }}",
        "name": "{{ extra_app.name }}{{ extra_volterra_site_id }}-{{ extra_env }}-devportal"
      }
    ],
    "loadbalancer_algorithm": "LB_OVERRIDE",
    "endpoint_selection": "LOCAL_PREFERRED"
  }
}