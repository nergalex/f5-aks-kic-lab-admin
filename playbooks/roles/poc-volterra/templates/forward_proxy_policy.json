{
  "metadata": {
    "name": "cloudbuilder",
    "namespace": "system",
    "labels": {
      "owner": "{{ extra_owner_email.split('@')[0] }}",
      "project": "cloudbuilder"
    },
    "annotations": {},
    "description": "NGINX workshop",
    "disable": false
  },
  "spec": {
    "any_proxy": {},
    "rule_list": {
      "rules": [
        {
          "metadata": {
            "name": "district-local",
            "description": "allow internal district communications",
            "disable": false
          },
          "action": "ALLOW",
          "all_sources": {},
          "dst_prefix_list": {
            "prefixes": [
              "10.0.0.0/8"
            ]
          },
          "no_http_connect_port": {}
        },
        {
          "metadata": {
            "name": "cross-management-public-ips",
            "description": "Cross Management Public IPs",
            "disable": false
          },
          "action": "ALLOW",
          "prefix_list": {
            "prefixes": ["10.0.0.0/8"]
          },
          "dst_prefix_list": {
            "prefixes": [
              "52.254.43.3/32"
            ]
          },
          "port_matcher": {
            "ports": ["80", "443"]
          },
          "no_http_connect_port": {}
        },
        {
          "metadata": {
            "name": "common-services",
            "description": "Common Services",
            "disable": false
          },
          "action": "ALLOW",
          "prefix_list": {
            "prefixes": ["10.0.0.0/8"]
          },
          "tls_list": {
            "tls_list": [
              {
                "suffix_value": "okta.com"
              },
              {
                "exact_value": "monip.io"
              },
              {
                "exact_value": "perdu.com"
              },
              {
                "exact_value": "identityauthority.ves.volterra.io"
              }
            ]
          },
          "no_http_connect_port": {}
        },
        {
          "metadata": {
            "name": "web-surfing-trusted",
            "description": "Trusted Categorized Services",
            "disable": false
          },
          "action": "ALLOW",
          "prefix_list": {
            "prefixes": ["10.0.0.0/8"]
          },
          "url_category_list": {
            "url_categories": [
              "REAL_ESTATE",
              "COMPUTER_AND_INTERNET_SECURITY",
              "REFERENCE_AND_RESEARCH",
              "SEARCH_ENGINES",
              "CONTENT_DELIVERY_NETWORKS",
              "INTERNET_COMMUNICATIONS"
            ]
          },
          "no_http_connect_port": {}
        },
        {
          "metadata": {
            "name": "web-surfing-untrusted",
            "description": "Untrusted Categorized Services",
            "disable": false
          },
          "action": "DENY",
          "prefix_list": {
            "prefixes": [
              "10.0.0.0/8"
            ]
          },
          "url_category_list": {
            "url_categories": [
              "UNCATEGORIZED",
              "FINANCIAL_SERVICES",
              "BUSINESS_AND_ECONOMY",
