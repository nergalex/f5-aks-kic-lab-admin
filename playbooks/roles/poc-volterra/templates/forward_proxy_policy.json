          "no_http_connect_port": {}
        },
        {
          "metadata": {
            "name": "web-surfing-malicious",
            "description": "Malicious Categorized Services",
            "disable": false
          },
          "action": "DENY",
          "prefix_list": {
            "prefixes": [
              "10.0.0.0/8"
