{
  "metadata": {
    "name": "nginx-agent",
    "namespace": "{{ extra_volterra_namespace }}",
    "labels": {
      "owner": "{{ extra_owner_email.split('@')[0] }}",
      "project": "cloudbuilder"
    },
    "description": "NGINX workshop",
    "disable": false
  },
  "spec": {
    "http_health_check": {
      "use_origin_server_name": {},
      "path": "/",
      "use_http2": null,
      "headers": {},
      "request_headers_to_remove": null
    },
    "timeout": 3,
    "interval": 15,
    "unhealthy_threshold": 1,
    "healthy_threshold": 3,
    "jitter_percent": 30
  }
}